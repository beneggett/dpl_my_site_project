<div class='row'>
  <div class='col-sm-6 col-sm-offset-3'>
    <div class='well'>
      <h1 class='text-center'> Sign In </h1>
      <%= simple_form_for :signin, url: create_session_path, input_html: {value: (params[:signin][:email] if params.has_key?(:signin)) }  do |f| %>
        <%= f.input :email%>
        <%= f.input :password %>
        <%= f.submit "Sign in", class: 'btn btn-success' %>
      <% end %>
    </div>
  </div>
</div>
